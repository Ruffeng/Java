public class SortOrdersByDate implements java.util.Comparator<Order> {

	@Override
	public int compare(Order o1, Order o2) {
		int result ; 
		if (o1.getDate().before(o2.getDate())) {
	            result = -1;
	        } else if (o1.getDate().after(o2.getDate())) {
	            result =  1;
	        } else {
	            result =  0;
	        }  
		System.out.println(result);
		return 0;
	}

}
