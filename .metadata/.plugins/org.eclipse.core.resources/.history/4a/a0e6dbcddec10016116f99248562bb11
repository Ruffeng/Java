        this.to = to;
        
    }

    public void read() {
        
    }
    
    public MessageStatus getStatus() {        
        return this.status;
    }

    public String toString() {                    
        return "";
    }
    
    public boolean equals(Object obj) {        
        return false; 
    }
}
